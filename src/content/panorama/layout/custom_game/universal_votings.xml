<root>
	<styles>
		        <include src="s2r://panorama/styles/dotastyles.vcss_c" />
        <include src="s2r://panorama/styles/popups/popup_settings.vcss_c" />
        <include src="s2r://panorama/styles/play.vcss_c" />
		<include src="file://{resources}/styles/custom_game/universal_votings.css" />
	</styles>
	<Panel class="voting dialog_hidden" hittest="true">
		<Panel class='dialog dialog_offset'>
			<Label class='title' text='TEAM SWITCH' id="titleLabel" />
			<Panel class='bar_border' >
				<Panel class='bar_inner' id='vote_timer' />
			</Panel>
			<Panel id='swap_info' >
				<Panel id='swapper_switch' class='player_info'>
					<Image id="picture"/>
				</Panel>
				<Label id="lineLabel" html='true'/>
			</Panel>
			<Label html="true" id="descriptionLabel" text='description'/>
			<Panel id='choice' class='choice' >
				<Panel class='isolation' >
					<Button id="acceptButton" class='accept'>
						<Label class="choice_label" text='Accept' />
					</Button>
				</Panel>
				<Panel class='isolation' >
					<Button id="declineButton" class='decline'>
						<Label class="choice_label" text='Decline' />
					</Button>
				</Panel>
			</Panel>
		</Panel>
	</Panel>
</root>